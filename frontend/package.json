{
  "name": "frontent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write \"**/*.+(js|json|ts|tsx|css|scss|sass|html)\"",
    "prettify": "npm run format && npm run lint:fix",
    "dev": "npm run prettify && next dev --turbopack",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "next": "15.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "prettier": "^3.6.2",
    "eslint-config-next": "15.4.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
